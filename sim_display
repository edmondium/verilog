 .timescale -9 -12;
v0x55a177a70350_0 .var "a", 7 0;
v0x55a177a707b0_0 .var "b", 7 0;
    .scope S_0x55a177a6ee20;
T_0 ;
    %vpi_call 2 25 "$dumpfile", "sim_display.vcd" {0 0 0};
    %vpi_call 2 26 "$dumpvars", 32'sb00000000000000000000000000000000, S_0x55a177a6ee20 {0 0 0};
    %vpi_call 2 27 "$monitor", "The value of a has changed to %d", v0x55a177a70350_0 {0 0 0};
    %pushi/vec4 10, 0, 8;
    %store/vec4 v0x55a177a70350_0, 0, 8;
    %pushi/vec4 1, 0, 8;
    %store/vec4 v0x55a177a707b0_0, 0, 8;
    %delay 10000, 0;
    %vpi_call 2 29 "$display", "The value of b is %d", v0x55a177a707b0_0 {0 0 0};
    %pushi/vec4 20, 0, 8;
    %store/vec4 v0x55a177a70350_0, 0, 8;
    %pushi/vec4 2, 0, 8;
    %store/vec4 v0x55a177a707b0_0, 0, 8;
    %delay 10000, 0;
    %vpi_call 2 31 "$display", "The value of b is %d", v0x55a177a707b0_0 {0 0 0};
    %pushi/vec4 30, 0, 8;
    %store/vec4 v0x55a177a70350_0, 0, 8;
    %pushi/vec4 3, 0, 8;
    %store/vec4 v0x55a177a707b0_0, 0, 8;
    %delay 10000, 0;
    %pushi/vec4 40, 0, 8;
    %store/vec4 v0x55a177a70350_0, 0, 8;
    %pushi/vec4 4, 0, 8;
    %store/vec4 v0x55a177a707b0_0, 0, 8;
    %delay 10000, 0;
    %vpi_call 2 34 "$display", "The value of b is %d", v0x55a177a707b0_0 {0 0 0};
    %pushi/vec4 50, 0, 8;
    %store/vec4 v0x55a177a70350_0, 0, 8;
    %pushi/vec4 5, 0, 8;
    %store/vec4 v0x55a177a707b0_0, 0, 8;
    %delay 10000, 0;
    %vpi_call 2 36 "$finish" {0 0 0};
    %end;
    .thread T_0;
# The file index is used to find the file name in the following table.
